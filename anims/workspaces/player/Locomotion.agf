     WeightPrimaryChain "isbitset(ArmIK, 1) && !IsTag(\"TagWeaponAction\")"
     WeightSecondaryChain "isbitset(ArmIK, 2) && !IsTag(\"TagMelee\") && !IsEvent(\"IK_Release_L\") && !IsTag(\"TagWeaponAction\") "
     WeaponDirLr "AimIKX"
     WeaponDirUd "AimIKY"
     WeaponRoll "AimIKZ"
     WeaponTransX "AimTransX"
     WeaponTransY "AimTransY"
     WeaponTransZ "float( (WeaponType != 4) || (WeaponType != 4) ) // do not apply offset if weapontype (RPG like)"\
     "*"\
